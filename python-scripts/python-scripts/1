from netmiko import ConnectHandler
from getpass import getpass

cisco4 = {
        'host' : "cisco4.lasthop.io",
        'username' : "pyclass",
        'password' : getpass(),
        'device_type' : "cisco_ios_ssh",
        'session_log' : "cisco4_ping.txt"
}

ssh = ConnectHandler(**cisco4)
print(ssh.find_prompt())

out = ssh.send_command_timing("ping", delay_factor = 1, strip_prompt = False, strip_command = False)
out += ssh.send_command_timing(" ",delay_factor = 1, strip_prompt = False, strip_command = False)
out += ssh.send_command_timing("8.8.8.8",delay_factor = 1, strip_prompt = False, strip_command = False)
out += ssh.send_command_timing(" ",delay_factor = 1, strip_prompt = False, strip_command = False)
out += ssh.send_command_timing(" ",delay_factor = 1, strip_prompt = False, strip_command = False
#out += ssh.send_command_timing(" ",delay_factor = 1, strip_prompt = False, strip_command = False)
#out += ssh.send_command_timing(" ",delay_factor = 1, strip_prompt = False, strip_command = False)
#out += ssh.send_command_timing(" ",delay_factor = 1, strip_prompt = False, strip_command = False)
print(out)

ssh.disconnect()


